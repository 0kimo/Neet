{
  "version": "2.0.0-beta",
  "nodeModulesDir": "auto",

  "fmt": {
    "include": ["./src/"],
    "useTabs": true,
    "indentWidth": 4
  },

  "tasks": {
    "fmt": "deno fmt",
    "lint": "deno lint",
    "start": "deno -A src/index.ts",
    "cache": "deno cache ./src/index.ts"
  },

  "imports": {
    "@std/dotenv": "jsr:@std/dotenv@^0.225.2",
    "@types/signale": "npm:@types/signale@^1.4.7",
    "discord.js": "npm:discord.js@^14.16.3",

    "lib": "./src/lib",
    "lib:exports": "./src/lib/exports.ts",
    "lib:logger": "./src/lib/Utils/Logger.ts",
    "mongoose": "npm:mongoose@^8.7.3"
  }
}
